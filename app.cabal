 name:               holbert
version:             0.1.0.0
synopsis:            An interactive proof assistant
category:            Web
build-type:          Simple
cabal-version:       >=1.10

executable app
  main-is:             Main.hs
  other-modules: StringRep Prop ProofTree Terms Unification Editor Rule Heading Paragraph Controller Item
  ghcjs-options:
